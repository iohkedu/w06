{
  "name": "Haskell",
  "dockerComposeFile": "docker-compose-haskell.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "haskell.haskell",
        "vigoo.stylish-haskell"
      ],
      "settings": {
        "stylishHaskell.commandLine": "/root/.local/bin/stylish-haskell"
      }
    }
  }
}
